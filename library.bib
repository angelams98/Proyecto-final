file = {:C$\backslash$:/Users/ANGELA/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Patil et al. - 2019 - Comparative biophysical characterization A screening tool for acetylcholinesterase inhibitors.pdf:pdf},
